{
  "name": "tilskudd-dashboard",
  "description": "Dashboard for tilskudd",
  "version": "1.5.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "next",
    "build": "next build && next export",
    "start": "next start",
    "deploy": "npm run build && now --static out --name dashboard-tilskudd && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "dashboard",
    "next",
    "status",
    "Telemark fylkeskommune",
    "tilskudd"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/tilskudd-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/tilskudd-dashboard/issues"
  },
  "homepage": "https://github.com/telemark/tilskudd-dashboard#readme",
  "now": {
    "name": "dashboard-tilskudd",
    "alias": [
      "tilskudd.dashboard.next.t-fk.no"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "coveralls": "3.0.2",
    "nsp": "3.2.1",
    "nyc": "13.0.1",
    "standard": "12.0.1"
  },
  "dependencies": {
    "axios": "0.18.0",
    "next": "6.1.1",
    "react": "16.5.0",
    "react-dom": "16.5.0"
  }
}
